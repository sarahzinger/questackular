{
    "name": "Questackular!",
    // "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "description": "Create custom quizzes containing curated questions and captivating content",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "icons": {
        "48":"questackular.png",
        "128": "questackular.png"
    },
    "homepage_url": "http://www.msn.com",
    "permissions": [
        "webRequest",
        "*://*/*",
        "cookies",
        "debugger",
        "declarativeContent",
        "identity",
        "tabs",
        "https://*/*",
        "http://*/*",
        "unlimitedStorage"
    ],
    "default_locale": "en",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["js/dependencies/jquery.min.js", "js/content.js"],
        "run-at": "document_idle"
    }],
    "browser_action": {
        "default_icon": "questackular.png",
        "default_title": "Questackular!",
        "default_popup": "popup.html"
    },
    "devtools_page": "devtools.html",
    "omnibox": {
        "keyword": "quest"
    }
    // "chrome_url_overrides": {
    //     "newtab": "popup.html"
    // }
}